@model IEnumerable<MaerskLine_TP034179.Models.Container>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


@*@Html.ActionLink("Create New Container", "New", "", new { @class = "btn btn-success" })*@

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ContainerID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContainerType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContainerWeight)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ContainerID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContainerType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContainerWeight)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.ContainerID }, new { @class = "btn btn-info btn-xs" })
                @Html.ActionLink("Delete", "Delete", new { id = item.ContainerID }, new { @class = "btn btn-danger  btn-xs" })
            </td>
        </tr>
    }

</table>
